<div class="all-container">
  <%= link_to "Back to profile", user_path(current_user) %>
  <%= simple_form_for current_user do |f| %>
  <%= f.input :username %>
  <%= f.input :first_name %>
  <%= f.input :second_name %>
  <%= f.country_select :nationality, class: "country-select", label: "Nationality" %>
  <%= f.input :bio %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit class: "btn btn-warning w-100"%>
  <% end %>


</div>
